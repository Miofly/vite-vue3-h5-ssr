    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "qs": "^6.11.0",
    "store2": "^2.14.2",
    "vant": "3.6.2",
    "vue": "3.2.39",
    "vue-router": "4.1.5",
    "vuex": "4.0.2",
    "windicss": "^3.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.0",

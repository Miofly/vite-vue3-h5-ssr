    "pinia": "^2.0.31",
    "qs": "^6.11.0",
    "store2": "^2.14.2",
    "vant": "4.0.11",
    "vue": "3.2.47",
    "vue-router": "4.1.6",
    "vuex": "4.1.0",
    "windicss": "^3.5.6"
